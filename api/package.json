{
  "name": "backend",
  "version": "1.2.0",
  "description": " A basic starter for an express.js API with Typescript",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "tsup:build": "npx prisma generate && tsup src/index.ts --format cjs,esm --dts --sourcemap --out-dir dist",
    "tsup:start": " npx prisma generate && tsup src/index.ts --format cjs,esm --dts --sourcemap --out-dir dist --watch",
    "start": "node ./dist/src/index.js",
    "build": "npx prisma generate && tsc && tsc-alias",
    "start:tsx": "tsx src/index.ts",
    "lint": "eslint --fix src test",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev --skip-generate",
    "db:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "gen:bulk":"tsx src/api/admin.ts"
  },
  "keywords": [],
  "repository": {
    "type": "git"
  },
  "license": "MIT",

  "dependencies": {
    "@apollo/server": "^4.12.0",
    "@pothos/core": "^4.5.1",
    "@pothos/plugin-prisma": "^4.5.0",
    "@pothos/plugin-relay": "^4.4.1",
    "@prisma/client": "^6.5.0",
    "bcrypt": "^5.1.1",
    "better-auth": "^1.2.5",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "graphql": "^16.10.0",
    "graphql-yoga": "^5.13.2",
    "helmet": "^8.1.0",
    "http": "0.0.1-security",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "nexus": "^1.3.0",
    "nexus-prisma": "^2.0.8",
    "nodemailer": "^6.10.0",
    "request-ip": "^3.3.0",
    "typescript": "^5.8.3",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/express-serve-static-core": "4.19.6",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.14.0",
    "@types/nodemailer": "^6.4.17",
    "@types/pg": "^8.11.11",
    "@types/request-ip": "^0.0.41",
    "@types/sendmail": "^1.4.7",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "eslint": "^9.24.0",
    "eslint-import-resolver-typescript": "^4.3.2",
    "eslint-plugin-import": "^2.31.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "prisma": "^6.5.0",
    "supertest": "^7.1.0",
    "tsc-alias": "^1.8.13",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "vitest": "^3.1.1"
  }
}
